# GitLab.com
Host gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

# GitHub.com
Host github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

Host git.ha.si
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitkey

# git.lsdevcloud.com
Host git.lsdevcloud.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/git.lsdevcloud.com
